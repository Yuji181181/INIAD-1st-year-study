    "import math\n",
    "\n",
    "# 対数部分の計算\n",
    "n1 = 10000\n",
    "n2 = 5000\n",
    "log_n1 = math.log(n1)\n",
    "log_n2 = math.log(n2)\n",
    "\n",
    "# 比率の計算\n",
    "time_ratio = (n2 * log_n2) / (n1 * log_n1)\n",
    "\n",
    "# リスト長 5000 の場合の時間の計算\n",
    "time_5000 = 1 * time_ratio  # 1ms が基準\n",
    "\n",
    "print(f\"{time_5000:.2f} ms\")"
   ]
