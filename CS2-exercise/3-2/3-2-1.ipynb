    "def simple_qsort(lst):\n",
    "    if len(lst) <= 1:\n",
    "        return lst\n",
    "    pivot = lst(0)\n",
    "    lst1 = [x for x in lst if x < pivot]\n",
    "    lst2 = [x for x in lst if x == pivot]\n",
    "    lst3 = [x for x in lst if x > pivot]\n",
    "\n",
    "    return simple_qsort(lst1)+lst2+simple_qsort(lst3)"
   ]
